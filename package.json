{
  "name": "mp-stream-notifier",
  "version": "0.1.0",
  "description": "Chrome extension for monitoring streams across Twitch, YouTube, and TwitCasting",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --watch --config webpack.config.js",
    "dev": "webpack --mode=development",
    "prod": "webpack --mode=production",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "test": "jest",
    "zip": "node scripts/zip.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tomozow/MPStreamNotifier.git"
  },
  "keywords": [
    "chrome-extension",
    "twitch",
    "youtube",
    "twitcasting",
    "stream",
    "notification"
  ],
  "author": "Tomozow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tomozow/MPStreamNotifier/issues"
  },
  "homepage": "https://github.com/Tomozow/MPStreamNotifier#readme",
  "devDependencies": {
    "@babel/core": "^7.24.1",
    "@babel/preset-env": "^7.24.1",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.8.1",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {}
}
